module FixIntegerType;
create OUT : MM refining IN : MM;

-- Zasada zmieniajÄ…ca typ danych z "intiger" na "Integer"
rule FixIntegerType {
    from
        c : MM!Column (c.type = 'intiger')
    to
        outC : MM!Column (
            type <- 'Integer'
        )
}